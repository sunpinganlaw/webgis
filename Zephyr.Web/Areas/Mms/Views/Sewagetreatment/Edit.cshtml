        <div class="grid_3 val"><input type="text" id="ServiceScope" name="ServiceScope" data-bind="value:form.ServiceScope" class="z-txt" /></div>
        <div class="grid_1 lbl">规划处理量</div>
        <div class="grid_3 val"><input type="text" id="PlanningCapacity" name="PlanningCapacity" data-bind="value:form.PlanningCapacity" class="z-txt" /></div>
        <div class="grid_1 lbl">实际处理量</div>
        <div class="grid_3 val "><input type="text" id="ActualCapacity" name="ActualCapacity" data-bind="value:form.ActualCapacity" class="z-txt" /></div>
       
        <div class="clear"></div>


