        <div class="grid_1 lbl">排入水体</div>
        <div class="grid_3 val"><input type="text" id="DischargeWater" name="DischargeWater" data-bind="value:form.DischargeWater" class="z-txt" /></div>
        <div class="grid_1 lbl">排放口数</div>
        <div class="grid_3 val "><input type="text" id="DischargeNumber" name="DischargeNumber" data-bind="value:form.DischargeNumber" class="z-txt" /></div>

        <div class="clear"></div>


        <div class="grid_1 lbl">排放口位置</div>
        <div class="grid_3 val"><input type="text" id="DischargeLoacation" name="DischargeLoacation" data-bind="value:form.DischargeLoacation" class="z-txt" /></div>
        <div class="grid_1 lbl">运行状况</div>
        <div class="grid_3 val"><input type="text" id="OperatingCondition" name="OperatingCondition" data-bind="value:form.OperatingCondition" class="z-txt" /></div>
        <div class="grid_1 lbl">处理成本</div>
        <div class="grid_3 val "><input type="text" id="TreatmentCost" name="TreatmentCost" data-bind="value:form.TreatmentCost" class="z-txt" /></div>

        <div class="clear"></div>

        <div class="grid_1 lbl ">资金来源</div>
        <div class="grid_3 val"><input type="text" id="FundsSources" name="FundsSources" data-bind="value:form.FundsSources" class="z-txt" /></div>
        <div class="grid_1 lbl">设施状态</div>
        <div class="grid_3 val"><input type="text" data-bind="comboboxValue:form.FacilityStatus,datasource:dataSource.FacilityStatus,comboboxReadOnly:readonly" class="z-txt easyui-combobox" /></div>
        <div class="grid_1 lbl">经度</div>
        <div class="grid_3 val"><input type="text" id="OrgY" name="OrgY" data-bind="value:form.OrgY" class="z-txt" /></div>

