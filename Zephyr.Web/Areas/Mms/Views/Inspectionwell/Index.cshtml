            }
            if ($('#item6').attr('checked')) 
            {  
                selecteditem6=$('input[name="item6"]:checked').val();

            }else{

                selecteditem6=0;
            }
          
            MyMapDiv.innerHTML="";
            var rows = $("#tbgrid").datagrid("getRows");
            adds(rows);
              

           

           
            
        });


        //// 初始化图表标签
        //var myChart = echarts.init(document.getElementById('chart'));
        //$(function () {
           
        //    $.ajax({
